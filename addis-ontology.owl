<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://trials.drugis.org/ontology#"
     xml:base="http://trials.drugis.org/ontology"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://trials.drugis.org/ontology#"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2004/10/xpath-datatypes#dayTimeDuration -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2004/10/xpath-datatypes#dayTimeDuration"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/source -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/source"/>
    


    <!-- http://trials.drugis.org/ontology#applied_in_epoch -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#applied_in_epoch">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Epoch"/>
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#StudyActivityApplication"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#applied_to_arm -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#applied_to_arm">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Arm"/>
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#StudyActivityApplication"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#dose_specification -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#dose_specification">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#DoseSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#has_activity_application -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#has_activity_application">
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#StudyActivity"/>
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#StudyActivityApplication"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#has_arm -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#has_arm">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Arm"/>
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#Study"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#has_drug -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#has_drug">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Drug"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#has_drug_treatment -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#has_drug_treatment">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#DrugTreatment"/>
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#TreatmentActivity"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#has_epochs -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#has_epochs">
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#Study"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#has_included_population -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#has_included_population"/>
    


    <!-- http://trials.drugis.org/ontology#has_publication -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#has_publication">
        <rdfs:range rdf:resource="http://purl.org/ontology/bibo/Document"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#is_subdivision_of -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#is_subdivision_of"/>
    


    <!-- http://trials.drugis.org/ontology#is_subgroup_of_arm -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#is_subgroup_of_arm">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Arm"/>
        <rdfs:subPropertyOf rdf:resource="http://trials.drugis.org/ontology#is_subdivision_of"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#is_subgroup_of_stratum -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#is_subgroup_of_stratum">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Stratum"/>
        <rdfs:subPropertyOf rdf:resource="http://trials.drugis.org/ontology#is_subdivision_of"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#of_group -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#of_group">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Group"/>
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#Measurement"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#of_moment -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#of_moment">
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#Measurement"/>
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#MeasurementMoment"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#of_variable -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#of_variable">
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#Measurement"/>
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#StudyVariable"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#registry -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#registry">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#StudyRegistration"/>
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#StudyRegistry"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#relative_to_anchor -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#relative_to_anchor">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#MeasurementMomentAnchor"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#relative_to_epoch -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#relative_to_epoch">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Epoch"/>
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#MeasurementMoment"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#treatment_dose -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#treatment_dose">
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#FixedDoseDrugTreatment"/>
        <rdfs:subPropertyOf rdf:resource="http://trials.drugis.org/ontology#dose_specification"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#treatment_max_dose -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#treatment_max_dose">
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#TitratedDoseDrugTreatment"/>
        <rdfs:subPropertyOf rdf:resource="http://trials.drugis.org/ontology#dose_specification"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#treatment_min_dose -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#treatment_min_dose">
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#TitratedDoseDrugTreatment"/>
        <rdfs:subPropertyOf rdf:resource="http://trials.drugis.org/ontology#dose_specification"/>
    </owl:ObjectProperty>
    


    <!-- http://trials.drugis.org/ontology#unit -->

    <owl:ObjectProperty rdf:about="http://trials.drugis.org/ontology#unit">
        <rdfs:range rdf:resource="http://trials.drugis.org/ontology#Unit"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://trials.drugis.org/ontology#duration -->

    <owl:DatatypeProperty rdf:about="http://trials.drugis.org/ontology#duration">
        <rdfs:range rdf:resource="http://www.w3.org/2004/10/xpath-datatypes#dayTimeDuration"/>
    </owl:DatatypeProperty>
    


    <!-- http://trials.drugis.org/ontology#has_start_date -->

    <owl:DatatypeProperty rdf:about="http://trials.drugis.org/ontology#has_start_date">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://trials.drugis.org/ontology#periodicity -->

    <owl:DatatypeProperty rdf:about="http://trials.drugis.org/ontology#periodicity">
        <rdfs:range rdf:resource="http://www.w3.org/2004/10/xpath-datatypes#dayTimeDuration"/>
    </owl:DatatypeProperty>
    


    <!-- http://trials.drugis.org/ontology#registration_id -->

    <owl:DatatypeProperty rdf:about="http://trials.drugis.org/ontology#registration_id">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://trials.drugis.org/ontology#StudyRegistration"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://trials.drugis.org/ontology#time_offset -->

    <owl:DatatypeProperty rdf:about="http://trials.drugis.org/ontology#time_offset">
        <rdfs:range rdf:resource="http://www.w3.org/2004/10/xpath-datatypes#dayTimeDuration"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#value -->

    <owl:DatatypeProperty rdf:about="&rdf;value"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/BibliographicResource -->

    <owl:Class rdf:about="http://purl.org/dc/terms/BibliographicResource">
        <owl:equivalentClass rdf:resource="http://purl.org/ontology/bibo/Document"/>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bibo/Document -->

    <owl:Class rdf:about="http://purl.org/ontology/bibo/Document"/>
    


    <!-- http://trials.drugis.org/ontology#Arm -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#Arm">
        <rdfs:subClassOf rdf:resource="http://trials.drugis.org/ontology#Group"/>
        <rdfs:comment>A group defined by randomization of a population.</rdfs:comment>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#ArmSubGroup -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#ArmSubGroup">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#is_subgroup_of_stratum"/>
                        <owl:onClass rdf:resource="http://trials.drugis.org/ontology#Stratum"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#is_subgroup_of_arm"/>
                        <owl:onClass rdf:resource="http://trials.drugis.org/ontology#Arm"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://trials.drugis.org/ontology#Group"/>
        <rdfs:comment>A subgroup of an arm defined as the intersection of the arm with one or more strata.</rdfs:comment>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#Dataset -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#Dataset">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/source"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/ontology/bibo/Document"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#DoseSpecification -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#DoseSpecification">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#unit"/>
                        <owl:onClass rdf:resource="http://trials.drugis.org/ontology#Unit"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#periodicity"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2004/10/xpath-datatypes#dayTimeDuration"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rdf;value"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="&xsd;double"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#Drug -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#Drug"/>
    


    <!-- http://trials.drugis.org/ontology#DrugTreatment -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#DrugTreatment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#has_drug"/>
                <owl:someValuesFrom rdf:resource="http://trials.drugis.org/ontology#Drug"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#Epoch -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#Epoch">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#duration"/>
                <owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2004/10/xpath-datatypes#dayTimeDuration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#FixedDoseDrugTreatment -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#FixedDoseDrugTreatment">
        <rdfs:subClassOf rdf:resource="http://trials.drugis.org/ontology#DrugTreatment"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#treatment_dose"/>
                <owl:someValuesFrom rdf:resource="http://trials.drugis.org/ontology#DoseSpecification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#FollowUpActivity -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#FollowUpActivity">
        <rdfs:subClassOf rdf:resource="http://trials.drugis.org/ontology#StudyActivity"/>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#Group -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#Group"/>
    


    <!-- http://trials.drugis.org/ontology#IncludedPopulation -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#IncludedPopulation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://trials.drugis.org/ontology#has_included_population"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:someValuesFrom rdf:resource="http://trials.drugis.org/ontology#Study"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://trials.drugis.org/ontology#Group"/>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#Measurement -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#Measurement">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#of_group"/>
                        <owl:someValuesFrom rdf:resource="http://trials.drugis.org/ontology#Group"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#of_moment"/>
                        <owl:someValuesFrom rdf:resource="http://trials.drugis.org/ontology#MeasurementMoment"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#of_variable"/>
                        <owl:someValuesFrom rdf:resource="http://trials.drugis.org/ontology#StudyVariable"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#MeasurementMoment -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#MeasurementMoment">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#relative_to_anchor"/>
                        <owl:onClass rdf:resource="http://trials.drugis.org/ontology#MeasurementMomentAnchor"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#relative_to_epoch"/>
                        <owl:onClass rdf:resource="http://trials.drugis.org/ontology#Epoch"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#time_offset"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2004/10/xpath-datatypes#dayTimeDuration"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#MeasurementMomentAnchor -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#MeasurementMomentAnchor"/>
    


    <!-- http://trials.drugis.org/ontology#RandomizationActivity -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#RandomizationActivity">
        <rdfs:subClassOf rdf:resource="http://trials.drugis.org/ontology#StudyActivity"/>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#ScreeningActivity -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#ScreeningActivity">
        <rdfs:subClassOf rdf:resource="http://trials.drugis.org/ontology#StudyActivity"/>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#Stratum -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#Stratum">
        <rdfs:subClassOf rdf:resource="http://trials.drugis.org/ontology#Group"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#is_subdivision_of"/>
                <owl:onClass rdf:resource="http://trials.drugis.org/ontology#IncludedPopulation"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A group defined as individuals matching certain criteria.</rdfs:comment>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#Study -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#Study"/>
    


    <!-- http://trials.drugis.org/ontology#StudyActivity -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#StudyActivity"/>
    


    <!-- http://trials.drugis.org/ontology#StudyActivityApplication -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#StudyActivityApplication">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#applied_in_epoch"/>
                        <owl:onClass rdf:resource="http://trials.drugis.org/ontology#Epoch"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#applied_to_arm"/>
                        <owl:onClass rdf:resource="http://trials.drugis.org/ontology#Arm"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#StudyRegistration -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#StudyRegistration">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#registry"/>
                        <owl:someValuesFrom rdf:resource="http://trials.drugis.org/ontology#StudyRegistry"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://trials.drugis.org/ontology#registration_id"/>
                        <owl:someValuesFrom rdf:resource="&xsd;string"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bibo/Document"/>
    </owl:Class>
    


    <!-- http://trials.drugis.org/ontology#StudyRegistry -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#StudyRegistry"/>
    


    <!-- http://trials.drugis.org/ontology#StudyVariable -->

    <owl:Class rdf:about="http://trials.drugis.org/ontology#StudyVariable"/>
    


